name: dockercompose7066115761865950308
services:
  bookify-idp:
    command:
      - start-dev
      - --import-realm
    container_name: Bookify.Identity
    environment:
      KEYCLOAK_ADMIN: admin
      KEYCLOAK_ADMIN_PASSWORD: admin
    image: quay.io/keycloak/keycloak:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "18080"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\Desktop\bookify\src\.containers\identity
        target: /opt/keycloak/data
        bind:
          create_host_path: true
      - type: bind
        source: D:\Desktop\bookify\src\.files\bookify-realm-export.json
        target: /opt/keycloak/data/import/realm.json
        bind:
          create_host_path: true
  bookify.api:
